{
  "name": "xhr-plus",
  "version": "1.5.0",
  "description": "xhr-plus",
  "keywords": [
    "XMLHttpRequest plus",
    "iframe upload",
    "subdomain proxy"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "homepage": "https://github.com/yiminghe/xhr-plus",
  "author": "yiminghe@gmail.com",
  "repository": "yiminghe/xhr-plus",
  "bugs": "https://github.com/yiminghe/xhr-plus/issues",
  "files": [
    "lib"
  ],
  "license": "MIT",
  "config": {
    "port": 8000
  },
  "scripts": {
    "build": "pack build",
    "prettier": "prettier --write \"{src,stories}/**/*.{js,tsx}\"",
    "pub": "npm run build && npm publish pkg",
    "deploy": "storybook-to-ghpages",
    "lint-staged": "lint-staged",
    "watch-tsc": "rc-tools run watch-tsc",
    "gh-pages": "rc-tools run gh-pages",
    "start": "node server",
    "lint": "rc-tools run lint",
    "karma": "rc-tools run karma",
    "saucelabs": "rc-tools run saucelabs",
    "test": "rc-tools run test",
    "chrome-test": "rc-tools run chrome-test",
    "coverage": "rc-tools run coverage"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "dependencies": {
    "modulex-querystring": "~1.0.5",
    "modulex-url": "~1.0.6",
    "object-assign": "4.x"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@pika/plugin-build-types": "^0.5.1",
    "@pika/plugin-build-web": "^0.5.1",
    "@pika/plugin-standard-pkg": "^0.5.1",
    "@pika/types": "^0.5.1",
    "co": "~4.6.0",
    "es6-promise": "3.x",
    "expect.js": "0.3.x",
    "jquery": "~1.12.4",
    "koa": "~1.2.1",
    "koa-body": "~1.4.0",
    "koa-compose": "~2.4.0",
    "lint-staged": "^9.2.1",
    "pre-commit": "1.x",
    "prettier": "^1.18.2",
    "rc-tools": "5.x"
  },
  "lint-staged": {
    "*.{tsx,js,jsx,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
